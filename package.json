{
    "name": "nextjs-sandbox",
    "license": "MIT",
    "version": "1.0.0",
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "check:type": "tsc",
        "check:lint": "eslint \"src/{**/*,*}.{ts,tsx}\"",
        "check:lint:fix": "eslint --fix \"src/{**/*,*}.{ts,tsx}\"",
        "check:prettier": "prettier --list-different \"{**/*,*}.{json,md,yml,yaml,scss,ts,tsx}\" \"!{*/build,node_modules,.next,.now}/**\"",
        "check:prettier:fix": "prettier --write \"{**/*,*}.{json,md,yml,yaml,scss,ts,tsx}\" \"!{*/.build,node_modules,.next,.now}/**\"",
        "test": "jest"
    },
    "dependencies": {
        "@sentry/nextjs": "^7.62.0",
        "next": "^13.4.7",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "sass": "^1.63.6"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^14.0.0",
        "@types/node": "^20.6.0",
        "@types/react": "^18.2.14",
        "@types/react-dom": "^18.2.4",
        "eslint": "^8.46.0",
        "eslint-config-next": "^13.4.7",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-simple-import-sort": "^10.0.0",
        "jest": "^29.5.0",
        "jest-environment-jsdom": "^29.5.0",
        "prettier": "2.8.8",
        "typescript": "^5.1.3"
    }
}
